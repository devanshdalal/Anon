{
  "manifest_version": 2,

  "name": "Anon",
  "description": "Automatically go Anonymous on quora questions as you follow them",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
     "default_title": "Anon"
  },

  "permissions": [
    "https://www.quora.com/*"
  ]
}